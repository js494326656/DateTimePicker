            android:name="com.lany.picker.samples.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lany.picker.samples.DatePickerActivity" />
        <activity android:name="com.lany.picker.samples.TimePickerActivity" />
        <activity android:name="com.lany.picker.samples.LanyPickerActivity" />
        <activity android:name="com.lany.picker.samples.CalendarViewActivity" />
        <activity android:name="com.lany.picker.samples.YmdhPickerActivity" />
        <activity android:name="com.lany.picker.samples.DateTimePickerActivity" />
    </application>

</manifest>